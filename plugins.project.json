{
	"name": "awm-plugins",
	"globIgnorePaths": [
		"**/package.json",
		"**/tsconfig.json",
		"out/client/main.client.luau",
		"out/server/main.server.luau"
	],
	"tree": {
		"$className": "Folder",
		"ServerScriptService": {
			"$className": "Folder",
			"TS": {
				"$path": "out/server"
			},
			"engine": {
				"$path": "out/engine/server"
			}
		},
		"ReplicatedStorage": {
			"$className": "Folder",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					}
				}
			},
			"TS": {
				"$path": "out/shared"
			},
			"engine": {
				"$path": "out/engine/shared"
			}
		},
		"StarterPlayer": {
			"$className": "Folder",
			"TS": {
				"$path": "out/client"
			},
			"engine": {
				"$path": "out/engine/client"
			}
		}
	}
}
